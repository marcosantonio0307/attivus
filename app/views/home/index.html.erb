<div class="row" style="margin-top: 0.5em"> <!-- begin logo bar -->
	<div class="col-lg-12 col-md-12 logo-bar">
		<div class="col-lg-3 col-md-3 col-sm-12" style="text-align: center;">
			<%= image_tag 'logo.jpg', :width=> '130', :height=> '100' %> 
		</div>

		<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 search">
			<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
				<%= text_field_tag :search, nil, placeholder: 'Busque seu produto aqui!', class: 'form-control' %>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-2" style="margin-left: -1.5em;">
				<span><%= image_tag 'search.png', :width=> '37' %></span>
			</div>	
		</div>
		</div>
		
	</div>
</div> <!-- end logo bar -->

<div class="row">
	<div class="col-lg-12 col-md-12 col-sm-12">
